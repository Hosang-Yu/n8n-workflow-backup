턴 추가\n      let submitMatch = str.match(/submitForm\\(this,'view',(\\d+)\\)/);\n      if (submitMatch) id = submitMatch[1];\n    }\n\n    // 날짜 정제\n    let dateStr = dates[index] || null;\n    if (dateStr) {\n      dateStr = dateStr.replace(/\\s+/g, '').replace(/\\.$/, '');\n      const dateMatch = dateStr.match(/(\\d{4})[.\\-](\\d{1,2})[.\\-](\\d{1,2})/);\n      if (dateMatch) {\n        const y = dateMatch[1];\n        const m = dateMatch[2].padStart(2, '0');\n        const d = dateMatch[3].padStart(2, '0');\n        dateStr = `${y}-${m}-${d}`;\n      } else {\n        dateStr = null;\n      }\n    }\n\n    const cleanTitle = (titles[index] || '')\n      .replace(/[\\[\\]「」<>\\\"']/g, '')\n      .trim();\n\n    return {\n      json: {\n        id: id,\n        title: cleanTitle,\n        date: dateStr\n      }\n    };\n  })\n  // ✅ 어제 날짜만 필터링\n  .filter(item => item.json.date === yesterdayStr);\n\nreturn result;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-2848,720],"id":"b6458f0c-8694-43fd-97d9-8949815e3e14","name":"금일 기준"},{"parameters":{"content":"사이트 주소\nhttps://www.kidp.or.kr/?menuno=1202","height":80,"width":660},"type":"n8n-nodes-base.stickyNote","typeVersion":1,"position":[-3936,608],"id":"9c2670ed-690d-4f3e-8e2f-53b847be978a","name":"Sticky Note"},{"parameters":{"jsCode":"let title = $input.first().json.title || '';\nlet announcement = $input.first().json.announcement\n\n// 괄호 또는 대괄호 안의 내용 제거\ntitle = title.replace(/[\\(\\[][^()\\[\\]]+[\\)\\]]/g, '').trim();\n\nreturn [\n  {\n    json: {\n      title,\n      announcement\n    }\n  }\n];\n\n\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-2480,496],"id":"923df968-3eeb-4874-8e86-b49dd5c19b50","name":"중복 자료 조회"},{"parameters":{"operation":"select","table":{"__rl":true,"value":"biz_info","mode":"list","cachedResultName":"biz_info"},"where":{"values":[{"column":"pblanc_nm","condition":"LIKE","value":"={{ $json.title }}"}]},"options":{}},"type":"n8n-nodes-base.mySql","typeVersion