":"Wait","type":"n8n-nodes-base.wait","typeVersion":1,"position":[-176,720],"webhookId":"654817e6-440a-4a5f-8354-5ed28fb67571"},{"parameters":{"jsCode":"const items = $('리스트 추출2').first().json;\nconst names = items.attach_file_name || [];\nconst addresses = items.attach_file_address || [];\n\nlet noticeFileName = null;\nlet noticeFileAddress = null;\nlet announcement_file_exist = false;\n\nconst extraFileNames = [];\nconst extraFilePaths = [];\n\n// -------------------- 현재 시각 (KST) --------------------\nconst now = new Date();\nconst kst = new Date(now.getTime() + 9 * 60 * 60 * 1000);\nconst formatted = kst.toISOString().replace('T', ' ').split('.')[0];\n\n// -------------------- 파일명 전처리 --------------------\nfunction normalizeFileName(name) {\n    return name?.replace(/[\\n\\r\\t]/g, \"\").replace(/\\(\\d+\\)$/, \"\").trim();\n}\n\n// -------------------- 주소에서 첨부번호 추출 --------------------\nfunction extractAttachNo(address) {\n    if (!address) return null;\n    const match = address.match(/submitForm\\(this,'down',(\\d+),'?'/);\n    return match ? match[1] : null;\n}\n\n// -------------------- 파일 분류 --------------------\nfor (let i = 0; i < names.length; i++) {\n    const name = normalizeFileName(names[i]);\n    const attachNo = extractAttachNo(addresses[i]);\n    if (!name || !attachNo) continue;\n\n    const fileUrl = attachNo; // 번호 자체를 URL로 사용\n\n    // zip 파일은 extra로 이동\n    if (name.toLowerCase().endsWith(\".zip\")) {\n        extraFileNames.push(name);\n        extraFilePaths.push(fileUrl);\n        continue;\n    }\n\n    // 공고문 파일 우선 선택\n    if (name.includes(\"공고\") && !noticeFileName) {\n        noticeFileName = name;\n        noticeFileAddress = fileUrl;\n        announcement_file_exist = true;\n    } else {\n        extraFileNames.push(name);\n        extraFilePaths.push(fileUrl);\n    }\n}\n\n// -------------------- 공고 파일 없는 경우 첫 번째 파일 대체 --------------------\nif (!noticeFileName && names.length > 0) {\n    for (let i = 0; i < names.length; i++) {\n        const candidateName = normalizeFileName(names[i]);\n        const candidateAttachNo = extractAttachNo(addresses[i]);\n        if (!candidateName.toLowerCase().endsWith(\".zip\") && candidateAttachNo) {\n            noticeFileName = candidateName;\n            noticeFileAddress = candidateAttachNo;\n            break;\n        }\n    }\n}\n\n// -------------------- 최종 리턴 --------------------\nreturn [{\n    json: {\n        fileName: noticeFileName,\n        fileAddress: noticeFileAddress,\n        file_nm: extraFileNames.join(\"@\"),\n        flpth_nm: extraFilePaths.join(\"@\"),\n        announcement_file_exist: announcement_file_exist,\n        creat_p_nttm: formatted,\n        created_ts: formatted\n    }\n}];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-1328,720],"id":"bdf97ddd-a9d6-4fa3-bfc0-82a183f46d2a","name":"pdf,첨부파일분리"},{"parameters":{"url":"https://www.kidp.or.kr/?menuno=1202","options":{"batching":{"batch":{"batchSize":10000,"batchInterval":10000}},"redirect":{"redirect":{}}}},"type":"n8n-nodes-base.httpRequest","typeVersion":4.2,"position":[-3408,720],"id":"bc95a92e-5832-4bb3-8031-344dd06b5527","name":"get detail","onError":"continueRegularOutput"},{"parameters":{"rule":{"interval":[{"triggerAtHour":23,"triggerAtMinute":30},{"triggerAtHour":11,"triggerAtMinute":30}]}},"type":"n8n-nodes-base.scheduleTrigger","typeVersion":1.2,"position":[-3680,720],"id":"901ad35f-c7e4-4b2c-8dd9-fd3d783d602b","name":"Schedule Trigger"}],"connections":{"Loop Over Items1":{"main":[[],[{"node":"중복 자료 조회","type":"main","index":0}]]},"리스트 추출2":{"main":[[{"node":"pdf,첨부파일분리","type":"main","index":0}]]},"금일 기준":{"main":[[{"node":"Loop Over Items1","type":"main","index":0}]]},"중복 자료 조회":{"main":[[{"node":"중복조회 MYSQL","type":"main","index":0}]]},"중복조회 MYSQL":{"main":[[{"node":"중복조회 IF","type":"main","index":0}]]},"중복조회 IF":{"main":[[{"node":"get detail1","type":"main","index":0}],[{"node":"Loop Over Items1","type":"main","index":0}]]},"리스트 추출":{"main":[[{"n