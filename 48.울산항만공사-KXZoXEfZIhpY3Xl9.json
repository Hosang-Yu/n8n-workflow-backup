 = String(kst.getSeconds()).padStart(2, '0');\nconst formatted = `${yyyy}-${mm}-${dd} ${hh}:${min}:${ss}`;\n\n// 파일명 전처리: 줄바꿈, 탭, 공백 제거 + (파일크기: ...) 제거\nfunction normalizeFileName(name) {\n    return name\n        ?.replace(/[\\n\\r\\t]/g, \"\")\n        .replace(/\\(파일크기:\\s*[\\d\\.]+[KMG]?B\\s*\\)$/i, \"\")\n        .trim();\n}\n\n// 주소에서 두 개 id 추출\nfunction extractFileIds(address) {\n    if (!address) return [null, null];\n    const match = address.match(/yhLib\\.file\\.download\\('([^']+)','([^']+)'\\)/);\n    if (match) {\n        return [match[1], match[2]];\n    }\n    return [address, null]; // 형식 안맞으면 그냥 첫 값만\n}\n\nfor (let i = 0; i < names.length; i++) {\n    const name = normalizeFileName(names[i]);\n    const [fileId1, fileId2] = extractFileIds(addresses[i]);\n    if (!name || !fileId1) continue;\n\n    // zip 파일은 무조건 extra 쪽으로 보냄\n    if (name.toLowerCase().endsWith(\".zip\")) {\n        extraFileNames.push(name);\n        extraFilePaths1.push(fileId1);\n        extraFilePaths2.push(fileId2);\n        continue;\n    }\n\n    if (name.includes(\"공고\") && !noticeFileName) {\n        noticeFileName = name;\n        noticeFileAddress1 = fileId1;\n        noticeFileAddress2 = fileId2;\n        announcement_file_exist = true;\n    } else {\n        extraFileNames.push(name);\n        extraFilePaths1.push(fileId1);\n        extraFilePaths2.push(fileId2);\n    }\n}\n\n// 공고 파일 없는 경우 첫 번째 파일로 대체\nif (!noticeFileName && names.length > 0) {\n    for (let i = 0; i < names.length; i++) {\n        const candidateName = normalizeFileName(names[i]);\n        const [candidateId1, candidateId2] = extractFileIds(addresses[i]);\n        if (!candidateName.toLowerCase().endsWith(\".zip\")) {\n            noticeFileName = candidateName;\n            noticeFileAddress1 = candidateId1;\n            noticeFileAddress2 = candidateId2;\n            break;\n        }\n    }\n}\n\nreturn [{\n    json: {\n        fileName: noticeFileName,\n        fileAddress1: noticeFileAddress1,\n        fileAddress2: noticeFileAddress2,\n        file_nm: extraFileNames.join(\"@\"),\n        flpth_nm1: extraFilePaths1.join(\"@\"),\n        flpth_nm2: extraFilePaths2.join(\"@\"),\n        announcement_file_exist: announcement_file_exist,\n        creat_p_nttm: formatted,\n        created_ts: formatted\n    }\n}];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-1072,-48],"id":"65fc6011-5d7b-4527-8359-11871506f34b","name":"pdf,첨부파일분리"},{"parameters":{"url":"https://www.upa.or.kr/portal/board/post/list.do","sendQuery":true,"queryParameters":{"parameters":[{"name":"bcIdx","value":"676"},{"name":"mid","value":"0601000000"},{"name":"token","value":"1757639295958"}]},"sendHeaders":true,"headerParameters":{"parameters":[{"name":"User-Agent","value":"Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:142.0) Gecko/20100101 Firefox/142.0"},{"name":"Accept","value":"text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8"},{"name":"Accept-Language","value":"ko-KR,ko;q=0.8,en-US;q=0.5,en;q=0.3"},{"name":"Accept-Encoding","value":"gzip, deflate, br, zstd"},{"name":"Referer","value":"https://www.upa.or.kr/main.do"},{"name":"Connection","value":"keep-alive"},{"name":"Cookie","value":"JSESSIONID=wd7D0ulDb5SEljVXY1h6PxmTfze5aRkIEqISfS1Ln8RP9a9glP9khEVc2aisc9cS.amV1c19kb21haW4vdXBh; popupClosedStatus=true"},{"name":"Upgrade-Insecure-Requests","value":"1"},{"name":"Sec-Fetch-Dest","value":"document"},{"name":"Sec-Fetch-Mode","value":"navigate"},{"name":"Sec-Fetch-Site","value":"same-origin"},{"name":"Sec-Fetch-User","value":"?1"},{"name":"Priority","value":"u=0, i"},{"name":"cookie","value":"JSESSIONID=wd7D0ulDb5SEljVXY1h6PxmTfze5aRkIEqISfS1Ln8RP9a9glP9khEVc2aisc9cS.amV1c19kb21haW4vdXBh;popupClosedStatus=true;"},{"name":"Cache-Control","value":"no-cache"},{"name":"cookie","value":"JSESSIONID=1VuS0zOjvZ5FxMzETZKTyRGZjSaesXVGEUvwYaqw16DEI5STllkwnatKDXcgblx5.amV1c19kb21haW4vc2VydmVyMQ==;popupCookie=done;"},{"name":"Content-Type","value":"application/x-www-form-urle