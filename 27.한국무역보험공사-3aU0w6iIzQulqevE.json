 = announcement?.trim() || null;\n    let dateStr = dates[index] || null;\n\n    // \"등록일 : 2025.09.08\" → \"2025-09-08\"\n    if (dateStr) {\n      const match = dateStr.match(/등록일\\s*:\\s*(\\d{4}\\.\\d{2}\\.\\d{2})/);\n      if (match) {\n        dateStr = match[1].replace(/\\./g, \"-\"); // yyyy.mm.dd → yyyy-mm-dd\n      }\n    }\n\n    return {\n      json: {\n        announcement: url,\n        title: (titles[index] || '')\n          .replace(/[\\[\\]]/g, '')\n          .replace(/\\s*새 글$/, '') // 끝에 붙는 \"새 글\" 제거\n          .trim(),\n        date: dateStr\n      }\n    };\n  })\n  // -------------------- 어제 날짜만 필터 --------------------\n  .filter(item => item.json.date === yesterdayStr);\n\nreturn result;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-2592,-48],"id":"aafb841f-6bec-4f22-9b8c-6b6cf0dca670","name":"금일 기준"},{"parameters":{"operation":"select","table":{"__rl":true,"value":"biz_info","mode":"list","cachedResultName":"biz_info"},"where":{"values":[{"column":"pblanc_nm","condition":"LIKE","value":"={{ $json.output.pblanc_nm }}"}]},"options":{}},"type":"n8n-nodes-base.mySql","typeVersion":2.4,"position":[1968,-544],"id":"58d8d1e5-7934-4aa5-9ff1-7bbbe653339f","name":"MySQL","alwaysOutputData":true,"credentials":{"mySql":{"id":"HSXhV0YEF0PlSVWX","name":"MySQL account"}}},{"parameters":{"conditions":{"options":{"caseSensitive":true,"leftValue":"","typeValidation":"strict","version":2},"conditions":[{"id":"2d3aee72-46af-4378-99b4-a974ed6c6c5c","leftValue":"={{ $json.pblanc_nm }}","rightValue":"","operator":{"type":"string","operation":"empty","singleValue":true}}],"combinator":"and"},"options":{}},"type":"n8n-nodes-base.if","typeVersion":2.2,"position":[2128,-544],"id":"37f4342a-d691-4c6f-a9cf-3ea1951e24c2","name":"If2"},{"parameters":{"jsCode":"// Loop over input items\nfor (const item of $input.all()) {\n  // 괄호 제거\n  if (item.json.pblanc_nm) {\n    item.json.pblanc_nm = item.json.pblanc_nm.replace(/[\\(\\[][^()\\[\\]]+[\\)\\]]/g, '').trim(