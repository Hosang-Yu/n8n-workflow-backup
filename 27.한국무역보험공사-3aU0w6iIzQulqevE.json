e(/[\\n\\r]/g, \"\").replace(/\\([\\d\\.]+[KMG]?B\\)/i, \"\").trim();\n    let address = addresses[i];\n    if (!name || !address) continue;\n\n    let fileUrl = address.startsWith(\"./\") ? `${baseUrl}${address.substring(1)}` : address;\n\n    const params = parseQueryString(fileUrl);\n\n    if (name.includes(\"공고\") && !noticeFileName) {\n        noticeFileName = name;\n        noticeFileAddress = fileUrl;\n        announcement_file_exist = true;\n        noticeFileQuery = params;\n    } else {\n        extraFileNames.push(name);\n        extraFilePaths.push(fileUrl);\n    }\n}\n\n// 공고 파일 없는 경우 첫 번째 파일로 대체\nif (!noticeFileName && names.length > 0 && addresses.length > 0) {\n    const firstName = names[0]?.replace(/[\\n\\r]/g, \"\").replace(/\\([\\d\\.]+[KMG]?B\\)/i, \"\").trim();\n    const firstAddress = addresses[0];\n    const firstUrl = firstAddress ? (firstAddress.startsWith(\"./\") ? `${baseUrl}${firstAddress.substring(1)}` : firstAddress) : null;\n\n    noticeFileName = firstName;\n    noticeFileAddress = firstUrl;\n    noticeFileQuery = firstUrl ? parseQueryString(firstUrl) : {};\n}\n\nreturn [{\n    json: {\n        fileName: noticeFileName,\n        fileAddress: noticeFileAddress,\n        file_nm: extraFileNames.join(\"@\"),\n        flpth_nm: extraFilePaths.join(\"@\"),\n        announcement_file_exist: announcement_file_exist,\n        bbs_id: noticeFileQuery.bbs_id || null,\n        atfile_sn: noticeFileQuery.atfile_sn || null,\n        data_ty_cd: noticeFileQuery.data_ty_cd || null,\n        ntt_sn: noticeFileQuery.ntt_sn || null,\n        creat_p_nttm: formatted,\n        created_ts: formatted\n    }\n}];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-752,-48],"id":"a2b1753d-e705-48ed-ab4e-5b5e9967f8c3","name":"pdf,첨부파일분리"},{"parameters":{"url":"={{ $('중복 자료 조회').item.json.announcement }}","options":{"redirect":{"redirect":{"followRedirects":false}},"response":{"response":{"fullResponse":true,"neverError":true}}}},"type":"n8n-nodes-